---
// @ts-ignore
import { Img } from "astro-imagetools/components";

const { quality, alt, src } = Astro.props;
---

<Img attributes={{ 
    img: {
        class: Astro.props.class,
        style: Astro.props.style || ""
    } 
}} quality={60 || quality} format="webp" placeholder="tracedSVG" loading="lazy" alt={alt || ""} src={src} />